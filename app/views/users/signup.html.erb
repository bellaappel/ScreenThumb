<h1> Sign Up! </h1>

<%= form_with model: @user do |f| %>
<div class="field" %>
 <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
<%= render partial: "form", locals: {user: @user} %>
  
   <div class="actions">
    <%= f.submit "Sign Up" %>
  </div>
<% end %>

<%= link_to 'Back', users_path %>


