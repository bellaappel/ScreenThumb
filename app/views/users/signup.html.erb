<h1> Sign Up! </h1>
<p id="notice"><%= notice %></p>

<%= form_with model: @user do |f| %>
<div class="field" %>
 <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= label_tag :name %>
    <%= text_field_tag "user[name]" %>
  </div>
<%= render partial: "form", locals: {user: @user} %>
  
   <div class="actions">
    <%= f.submit "Sign Up" %>
  </div>
<% end %>

<%= link_to 'Back', root_path %>


