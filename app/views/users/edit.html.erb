<h1>Edit User</h1>
<%= form_with model: @user do |f| %>
<%= render partial: "users/form", locals: {user: @user} %>
  <div class="actions">
    <%= f.submit "Edit" %>
  </div>
<% end %>
<%= button_to "Delete Account", @user, method: 'delete', data: 
    {confirm: "Delete account? Action cannot be undone." } %>
<%= link_to 'Back', user_path %>

